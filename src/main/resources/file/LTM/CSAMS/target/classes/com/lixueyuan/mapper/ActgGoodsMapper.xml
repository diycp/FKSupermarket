<mapper namespace="com.lixueyuan.mapper.ActgGoodsMapper">
	
	<!-- 添加活动物品 int addActivityGoods(ActgGoods actgGoods); -->
	<insert id="addActivityGoods" parameterType="actgGoods" >
		insert into
		t_actggoods(g_id,AC_id, AL_num)
		values(#{g_id},#{ac_id},#{al_num})
	</insert>

